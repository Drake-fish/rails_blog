<h1>Log in</h1>


<%= form_for(:session, :html => {class: "login-form", role: "form" }, url: login_path) do |f| %>
  <p>
    <%= f.label :email %><br/>
    <%= f.email_field :email, placeholder: "email" %>
  </p>
  <p>
    <%= f.label :password %><br/>
    <%= f.password_field :password, placeholder: "password" %>
  </p>
  <p>
    <%= f.submit "Login"%>
  </p>
  <%= link_to "Back", posts_path %>
<% end %>
